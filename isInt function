public boolean isInt(String str) {
     String s = symbolTableTree.getCurrentScope().getDataTable().find_id_by_name(str);

     if (str.contains("'") || str.equals("true") || str.equals("false")) {
      return false;
     }
     if ((!s.equals("not found")) && s.equals("int")) {
      return true;
     }
     else return false;
    }
